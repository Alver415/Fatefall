<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="root" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.alver.fatefall.poker.plugin.template.PokerCardFrontController"
           stylesheets="@PokerCard.css">

    <Label text="${controller.rank}" translateX="-80" translateY="-120"/>
    <Label text="${controller.suit}" translateX="-80" translateY="-50"/>

    <Label text="${controller.suit}" translateX="80" translateY="50" rotate="180"/>
    <Label text="${controller.rank}" translateX="80" translateY="120" rotate="180"/>

</StackPane>
